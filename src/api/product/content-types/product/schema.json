{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "item",
      "required": true
    },
    "description": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "price": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 0
    },
    "stock": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 1
    },
    "minOrder": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 1
    },
    "deliveryTime": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 24
    },
    "discount": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 0
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "product"
    }
  }
}
